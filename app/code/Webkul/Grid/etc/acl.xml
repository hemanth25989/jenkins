				<resource id="Webkul_Grid::grid_team" title="Team List" sortOrder="10">
					<resource id="Webkul_Grid::add_row_team" title="Add Team" sortOrder="10" />
					<resource id="Webkul_Grid::row_team_delete" title="Team Mass Delete" sortOrder="20" />
