<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Webkul_Grid::grid_list" title="Developer List" sortOrder="10">
					<resource id="Webkul_Grid::add_row" title="Add Developer" sortOrder="10" />
					<resource id="Webkul_Grid::row_data_delete" title="Mass Delete" sortOrder="20" />
